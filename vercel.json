{
  "version": 2,
  "buildCommand": "npm install && npx prisma generate",
  "installCommand": "npm install",
  "env": {
    "DATABASE_URL": "@database_url"
  },
  "functions": {
    "api/index.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.+)",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/public/index2.html"
    }
  ]
}
