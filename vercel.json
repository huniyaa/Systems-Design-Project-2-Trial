{
  "version": 2,
  "buildCommand": "npm install && npx prisma generate",
  "installCommand": "npm install",
  "env": {
    "DATABASE_URL": "@database_url"
  },
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/public/index2.html"
    }
  ]
}
