{
  "version": 2,
  "buildCommand": "npm install && npx prisma generate",
  "installCommand": "npm install",
  "env": {
    "DATABASE_URL": "@database_url"
  },
  "rewrites": [
    { "source": "/api/test", "destination": "/api/test.js" },
    { "source": "/api/trips", "destination": "/api/trips.js" },
    { "source": "/api/cities", "destination": "/api/cities.js" },
    { "source": "/api/activities", "destination": "/api/activities.js" }
  ]
}
