{
  "version": 2,
  "buildCommand": "npm install && npx prisma generate",
  "installCommand": "npm install",
  "env": {
    "DATABASE_URL": "@database_url"
  },
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
